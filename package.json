{
  "name": "time",
  "description": "\"time.h\" bindings for Node.js",
  "keywords": [
    "date",
    "time",
    "time.h",
    "timezone",
    "setTimezone",
    "getTimezone"
  ],
  "version": "0.12.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/TooTallNate/node-time.git"
  },
  "author": "Joe Mainwaring <jm@theaccordance.com> (http://joemainwaring.com)",
  "license": "MIT",
  "contributors": [
    {
      "name": "Diogo Resende",
      "email": "dresende@thinkdigital.pt"
    },
    {
      "name": "Nathan Rajlich",
      "email": "nathan@tootallnate.net"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "build:clear": "rm -rf dist/*",
    "nuke": "rm -rf node_modules/* dist/* coverage/*",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "bindings": "^1.2.0",
    "debug": "^2.2.0",
    "nan": "^2.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.2.2",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
